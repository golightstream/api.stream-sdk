{
  "ignore": [
    "*.md",
    "**/*.md",
    "*.lock",
    "*.bin",
    "*.jar",
    "sdk/js/src/version/index.js",
    "sdk/js/lib/**",
    "sdk/java/bin/**",
    "sdk/java/build/**",
    "sdk/java/gradle/**",
    "sdk/java/.gradle/**",
    "sdk/java/.idea/**",
    "sdk/java/gradlew",
    "sdk/java/gradlew.bat",
    "sdk/js/lib/**",
    "deps/**"
  ],
  "license": "build/LICENSE.snippet.txt",
  "licenseFormats": {
    "js|ts|tsx|jsx|java|npmrc": {
      "prepend": "",
      "append": "",
      "file": "build/LICENSE.snippet-js.txt"
    },
    "yml": {
      "append": "",
      "eachLine": {
        "prepend": "# "
      }
    }
  },
  "trailingWhitespace": "TRIM"
}