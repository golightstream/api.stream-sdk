{
  "ignore": [
    "**/*.md",
    "**/*.lock",
    "**/*.bin",
    "**/*.jar",
    "**/*.gradle",
    "sdk/js/src/version/index.js",
    "**/lib/**",
    "**/bin/**",
    "**/build/**",
    "**/gradle/**",
    "**/.gradle/**",
    "**/.idea/**",
    "**/gradlew",
    "**/gradlew.bat",
    "deps/**"
  ],
  "license": "build/LICENSE.snippet.txt",
  "licenseFormats": {
    "js|ts|tsx|jsx|java|npmrc": {
      "prepend": "",
      "append": "",
      "file": "build/LICENSE.snippet-js.txt"
    },
    "yml": {
      "append": "",
      "eachLine": {
        "prepend": "# "
      }
    }
  },
  "trailingWhitespace": "TRIM"
}